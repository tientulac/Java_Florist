                    $.ajax({
                        url: '../CartItem/InsertCartItem?userId=' + user_id + '&bouquetiId=' + bouqueti_id,
                        type: 'GET',
                        success: function (result) {
                            if (result) {
                                toastr.success('Add to cart successfully !');
