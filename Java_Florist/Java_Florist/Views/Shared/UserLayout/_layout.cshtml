        <script>
            var check = false;
            var count_cart = 0;
            var user_id = 0;

            getCountCart = () => {
                $.ajax({
                    url: '/CartItem/GetCountCart?userId=' + user_id,
                    type: 'GET',
                    success: function (result) {
                        if (result) {
                            $('#countCart').html('(' + result + ')');
                        }
                        else {
                            toastr.warning('Failed !');
                            $('.modal').modal('hide');
                        }
                    }
                });
            }

            $(document).ready(function () {
                $(".fancybox").fancybox({
                    openEffect: "none",
                    closeEffect: "none"
                });

                $(".zoom").hover(function () {

                    $(this).addClass('transition');
                }, function () {

                    $(this).removeClass('transition');
                });

                if (localStorage.getItem("UserInfo")) {
                    check = true;
                    var info = JSON.parse(localStorage.getItem("UserInfo"));
                    user_id = info.UserId;
                    $('#userIcon').show();
                    $('#accountInfo').append('<i class="fa-solid fa-user"></i> ' + info.UserName);
                    getCountCart();
                }
            });

            login = () => {
                var userInfo = {
                    UserId: 2,
                    Admin: true,
                    UserName: 'admin'
                };

                toastr.success('Login successfully !');
                $('.modal').modal('hide');
                localStorage.setItem("UserInfo", JSON.stringify(userInfo));

                if (localStorage.getItem("UserInfo") && !check) {
                    var info = JSON.parse(localStorage.getItem("UserInfo"));
                    $('#userIcon').show();
                    $('#accountInfo').html('<i class="fa-solid fa-user"></i> ' + info.UserName)
                }
            }

            logout = () => {
                localStorage.removeItem("UserInfo");
                window.location.reload();
            }

            addToCart = (bouqueti_id) => {
                if (!localStorage.getItem("UserInfo")) {
                    toastr.warning('You need to login for adding to cart !');
                }
                else {
                    $.ajax({
                        url: '../CartItem/InsertCartItem?userId=' + user_id + '&bouquetiId=' + bouqueti_id,
                        type: 'GET',
                        success: function (result) {
                            if (result) {
                                toastr.success('Add to cart successfully !');
                                getCountCart();
                            }
                            else {
                                toastr.warning('Failed !');
                                $('.modal').modal('hide');
                            }
                        }
                    });
                }
            }

            openCartInfo = () => {
                var totalPrice = 0;
                if (!localStorage.getItem("UserInfo")) {
                    $('#notLogin').show();
                    $('.infoCart').hide();
                }
                else {
                    $.ajax({
                        url: '../CartItem/GetCartItem?userId=' + user_id,
                        type: 'GET',
                        success: function (result) {
                            if (result.data.length > 0) {
                                console.log(result);
                                var _html = '';
                                result.data.forEach(x => {
                                    var str = `<div class="card mb-3">
                                                                <div class="card-body">
                                                                    <div class="d-flex justify-content-between">
                                                                        <div class="d-flex flex-row align-items-center">
                                                                            <div>
                                                                                <img src="`+ x.Image + `"
                                                                                        class="img-fluid rounded-3" alt="Shopping item" style="width: 65px;">
                                                                            </div>
                                                                            <div class="ms-3">
                                                                                <h5 style="margin-left: 10px;">`+ x.BouquetiName + `</h5>
                                                                                <p style="margin-left: 10px;" class="small mb-0">`+ x.Desc + `</p>
                                                                            </div>
                                                                        </div>
                                                                        <div class="d-flex flex-row align-items-center">
                                                                            <div style="width: 50px;">
                                                                                <h5 class="fw-normal mb-0">`+ x.TotalCount + `</h5>
                                                                            </div>
                                                                            <div style="width: 80px;">
                                                                                <h5 class="mb-0">$ `+ x.Price + `</h5>
                                                                            </div>
                                                                            <a href="#!" style="color: #cecece;" onclick="deleteCartItem(`+ x.CartItemId + `)"><i class="fas fa-trash-alt"></i></a>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="card-body">
                                                                    <input type="text" class="form-control" id="messge-`+ x.BouquetiId +`" value="" placeholder="Input your message wanto send"/>
                                                                </div>
                                                            </div>`;
                                    _html += str;
                                    totalPrice = totalPrice + (parseInt(x.Price) * parseInt(x.TotalCount));
                                })
                                $('#displayCartItem').html(_html);
                                $('#totalPricePay').html(totalPrice + 20);
                                $('#subTotal').html(totalPrice);
                            }
                            else {
                                toastr.warning('Failed !');
                                $('.modal').modal('hide');
                            }
                        }
                    });
                    $('#notLogin').hide();
                    $('.infoCart').show();
                }
            }

            deleteCartItem = (id) => {
                var totalPrice = 0;
                $.ajax({
                    url: '../CartItem/Delete?cartItemId=' + id,
                    type: 'GET',
                    success: function (result) {
                        if (result) {
                            $.ajax({
                                url: '../CartItem/GetCartItem?userId=' + user_id,
                                type: 'GET',
                                success: function (result) {
                                    if (result.data.length > 0) {
                                        console.log(result);
                                        var _html = '';
                                        result.data.forEach(x => {
                                            var str = `<div class="card mb-3">
                                                                <div class="card-body">
                                                                    <div class="d-flex justify-content-between">
                                                                        <div class="d-flex flex-row align-items-center">
                                                                            <div>
                                                                                <img src="`+ x.Image + `"
                                                                                        class="img-fluid rounded-3" alt="Shopping item" style="width: 65px;">
                                                                            </div>
                                                                            <div class="ms-3">
                                                                                <h5 style="margin-left: 10px;">`+ x.BouquetiName + `</h5>
                                                                                <p style="margin-left: 10px;" class="small mb-0">`+ x.Desc + `</p>
                                                                            </div>
                                                                        </div>
                                                                        <div class="d-flex flex-row align-items-center">
                                                                            <div style="width: 50px;">
                                                                                <h5 class="fw-normal mb-0">`+ x.TotalCount + `</h5>
                                                                            </div>
                                                                            <div style="width: 80px;">
                                                                                <h5 class="mb-0">$ `+ x.Price + `</h5>
                                                                            </div>
                                                                            <a href="#!" style="color: #cecece;" onclick="deleteCartItem(`+ x.CartItemId + `)"><i class="fas fa-trash-alt"></i></a>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <input type="text" class="form-control" id="messge-`+ x.BouquetiId +`" value="" />
                                                            </div>`;
                                            _html += str;
                                            totalPrice = totalPrice + (parseInt(x.Price) * parseInt(x.TotalCount));
                                        })
                                        $('#displayCartItem').html(_html);
                                        $('#totalPricePay').html(totalPrice + 20);
                                        $('#subTotal').html(totalPrice);
                                    }
                                    else {
                                        $('.modal').modal('hide');
                                    }
                                }
                            });
                        }
                        else {
                            toastr.warning('Failed !');
                        }
                    }
                });
            }

            placeOrder = () => {
                var req = {
                    UserId: user_id,
                    Status: 1,
                    Address_From: '',
                    Address_To: '',
                    TypePayment: '',
                    TimeDelivery: ''
                }
            }

            checkPayment = () => {
                alert($('#checkPaymentType').val())
            }
        </script>
</body>
</html>
