                                                                                <a href="#!" style="color:#308409;;margin-right:20px;font-size:25px;font-weight:bold" onclick="updateCountCart(${x.CartItemId}, '#count_${x.CartItemId}')"><i class="fa fa-check" aria-hidden="true"></i></a>
                                                                                <a href="#!" style="color: #cecece;" onclick="deleteCartItem(`+ x.CartItemId + `)"><i class="fas fa-trash-alt"></i></a>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                    <div class="card-body">
                                                                        <input type="text" class="form-control" id="message-`+ x.BouquetiId + `" value="" placeholder="Input your message wanto send"/>
                                                                    </div>
                                                                </div>`;
                                    _html += str;
                                    totalPrice = totalPrice + (parseInt(x.Price) * parseInt(x.TotalCount));
                                })
                                $('#displayCartItem').html(_html);
                                $('#totalPricePay').html(totalPrice + 20);
                                $('#subTotal').html(totalPrice);
                            }
                            else {
                                toastr.warning('Failed !');
                                $('.modal').modal('hide');
                            }
                        }
                    });
                    $('#notLogin').hide();
                    $('.infoCart').show();
                }
            }

            updateCountCart = (cartItemId, id_count) => {
                var count_update = $(id_count).val();
                var totalPrice = 0;
                $.ajax({
                    url: '../CartItem/UpdateCountCart?cartItemId=' + cartItemId + '&count=' + count_update,
                    type: 'GET',
                    success: function (result) {
                        if (result) {
                            toastr.success('Update count successfully !');
                            getCountCart();
