                                url: '../CartItem/GetCartItem?userId=' + user_id,
                                type: 'GET',
                                success: function (result) {
                                    if (result) {
                                        result.data.forEach(x => {
                                            totalPrice = totalPrice + (parseInt(x.Price) * parseInt(x.TotalCount));
                                        })
                                        $('#totalPricePay').html(totalPrice + 20);
                                        $('#subTotal').html(totalPrice);
